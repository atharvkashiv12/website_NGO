<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>RegisterChecker</servlet-name>
        <servlet-class>controller1.RegisterChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegister1</servlet-name>
        <servlet-class>controller1.AdminRegister1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApprovedNgo</servlet-name>
        <servlet-class>controller1.ApprovedNgo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller1.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginChecker1</servlet-name>
        <servlet-class>controller1.LoginChecker1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterChecker</servlet-name>
        <url-pattern>/RegisterChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegister1</servlet-name>
        <url-pattern>/AdminRegister1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApprovedNgo</servlet-name>
        <url-pattern>/ApprovedNgo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginChecker1</servlet-name>
        <url-pattern>/LoginChecker1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
